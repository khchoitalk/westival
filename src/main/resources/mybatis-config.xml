	<mappers>
		<mapper resource="mappers/member-mapper.xml" />
		<mapper resource="mappers/community-mapper.xml" />
		<mapper resource="mappers/festival-mapper.xml" />
		<mapper resource="mappers/notice-mapper.xml" />
		<mapper resource="mappers/qna-mapper.xml" />
		<mapper resource="mappers/ticket-mapper.xml" />
		<mapper resource="mappers/ticketOption-mapper.xml" />
		<mapper resource="mappers/scrap-mapper.xml" />
		<mapper resource="mappers/admin-mapper.xml" />
	</mappers>
</configuration>

