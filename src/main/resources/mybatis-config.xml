		<typeAlias type="org.kh.westival.festival.model.vo.Festival" alias="Festival" />
		<typeAlias type="org.kh.westival.festival.model.vo.FestivalReply" alias="FestivalReply" />
		<typeAlias type="org.kh.westival.festival.model.vo.Recommend" alias="Recommend" />
